<launch>
  <param name="/use_sim_time" value="true"/>
  <node pkg="second_project" type = "tf_publisher" name = "tf_publisher"/>
  <node pkg="tf2_ros" type="static_transform_publisher" name="base_footprint_tf" args="0 0 0 0 0 0 1 t265 base_footprint" />
  <node pkg="tf2_ros" type="static_transform_publisher" name="velodyne_tf" args="0 0 0 0 0 0 1 t265 velodyne" />
  <node pkg="tf2_ros" type="static_transform_publisher" name="laser_tf" args="0 0 0 0 0 0 1 t265 laser" />
  
  <!-- Slam toolbox-->
  <node pkg="slam_toolbox" type="async_slam_toolbox_node" name="slam_toolbox" output="screen">
    <rosparam command="load" file="$(find second_project)/cfg/st_config_laser.yaml" />
  </node>

  <!--  rviz  -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find second_project)/config_mapping.rviz"/>

</launch>

